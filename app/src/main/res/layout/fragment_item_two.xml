<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="8dp"
    android:layout_marginLeft="16dp"
    tools:context="com.example.chinatip.timetracker.ItemThreeFragment">

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/codinP"
        android:layout_alignEnd="@+id/textView17"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignRight="@+id/textView17"
        android:layout_alignStart="@+id/textView17"
        android:layout_alignTop="@+id/goal3"
        android:text="%"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/total"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView32"
        android:layout_alignLeft="@+id/codinP"
        android:layout_alignStart="@+id/codinP"
        android:layout_alignTop="@+id/textView32"
        android:elevation="1dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_width="50dp" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/goal3"
        android:layout_alignStart="@+id/goal3"
        android:layout_below="@+id/goal3"
        android:layout_marginTop="50dp"
        android:text="Total"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="120dp"
        android:text="Date"
        android:textSize="30sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/goal1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/date"
        android:layout_marginTop="44dp"
        android:text="Entertainment"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/goal2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/goal1"
        android:layout_alignLeft="@+id/goal1"
        android:layout_alignRight="@+id/goal1"
        android:layout_alignStart="@+id/goal1"
        android:layout_below="@+id/goal1"
        android:layout_marginTop="34dp"
        android:text="Studying"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/goal3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/goal2"
        android:layout_alignStart="@+id/goal2"
        android:layout_below="@+id/studyingP"
        android:layout_marginTop="20dp"
        android:text="Coding"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/studyingP"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/goal2"
        android:layout_alignBottom="@+id/goal2"
        android:layout_alignLeft="@+id/entertainmentP"
        android:layout_alignStart="@+id/entertainmentP"
        android:elevation="1dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_width="50dp" />

    <TextView
        android:id="@+id/entertainmentP"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/goal1"
        android:layout_alignBottom="@+id/goal1"
        android:layout_toEndOf="@+id/date"
        android:layout_toRightOf="@+id/date"
        android:elevation="1dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_width="55dp" />

    <TextView
        android:id="@+id/codinP"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/goal3"
        android:layout_alignBottom="@+id/goal3"
        android:layout_alignLeft="@+id/studyingP"
        android:layout_alignStart="@+id/studyingP"
        android:elevation="1dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_width="50dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/studyingP"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignStart="@+id/textView18"
        android:layout_alignTop="@+id/goal2"
        android:layout_toEndOf="@+id/textView17"
        android:text="%"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/total"
        android:layout_alignLeft="@+id/textView35"
        android:layout_alignStart="@+id/textView35"
        android:layout_alignTop="@+id/total"
        android:text="minutes"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/goal1"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@+id/entertainmentP"
        android:layout_toRightOf="@+id/entertainmentP"
        android:text="%"
        android:textSize="18sp" />

    <ToggleButton
        android:id="@+id/dailyResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/goal1"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/goal1"
        android:layout_marginTop="53dp"
        android:text="Daily"
        android:textOff="Daily"
        android:textOn="Daily" />

    <ToggleButton
        android:id="@+id/weeklyResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/dailyResult"
        android:layout_alignEnd="@+id/textView18"
        android:layout_alignRight="@+id/textView18"
        android:text="ToggleButton"
        android:textOff="Weekly"
        android:textOn="Weekly" />

</RelativeLayout>
